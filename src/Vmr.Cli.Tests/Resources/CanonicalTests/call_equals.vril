.method _equals:
    ldstr "Equals"
    ret

.method _not_equals:
    ldstr "Not equals"
    ret

.method _start:
    ldc.i4 0
    ldc.i4 1
    ceq
    brtrue _equals

_not_equals:
    call _not_equals
    br _end

_equals:
    call _equals

_end:
    nop


